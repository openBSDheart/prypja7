header
	
    number -- {int}
	previous_hash -- {byte[]}
	data_hash -- {byte[]}

data

	data -- {array}
		signature -- {byte[]}
		payload
			header -- {Header} IST:

                channel_header
                    type -- {string}
                    version -- {int}
                    timestamp -- {time}
                    channel_id -- {string}
                    tx_id -- {string}
                    epoch -- {int}
                
                signature_header -- {SignatureHeader} IST: (neu ab Version 1.1?)
                        
                        creator
                            Mspid -- {string}
                            IdBytes -- {byte[]}
                        nonce -- {byte[]}

		    data -- {ConfigEnvelope *NUR TEIL EINES CONFIG BLOCKS | Transaction}
                actions
                    
                    header
                        
                        creator
